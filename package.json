{
  "name": "saproto",
  "version": "0.0.2",
  "description": "S.A. Proto Main Website",
  "main": "./public/index.php",
  "repository": {
    "type": "git",
    "url": "https://github.com/saproto/saproto.git"
  },
  "keywords": [
    "SAPROTO",
    "Utwente",
    "University of Twente",
    "Laravel",
    "JS",
    "SASS",
    "CSS",
    "Grunt"
  ],
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/saproto/saproto/issues"
  },
  "homepage": "https://github.com/saproto/saproto/tree/master/example",
  "private": false,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.4.1",
    "bootstrap": "^4.1.0",
    "bootstrap-slider": "^10.2.1",
    "fontawesome-iconpicker": "^3.1.0",
    "js-cookie": "^2.2.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "popper.js": "^1.14.4",
    "select2": "^4.0.5",
    "select2-bootstrap4-theme": "^1.0.0",
    "simplemde": "^1.11.2",
    "socket.io-client": "^2.1.1",
    "tempusdominus-bootstrap-4": "^5.1.2",
    "tempusdominus-core": "^5.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.4",
    "eslint": "^1.9.0",
    "eslint-config-eslint": "^1.0.1",
    "grunt": "^0.4.5",
    "grunt-autoprefixer": "^3.0.0",
    "grunt-bump": "^0.3.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-concat": "~0.5.1",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-cssmin": "^0.13.0",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-contrib-watch": "~0.6.1",
    "grunt-sass": "^1.0.0",
    "load-grunt-tasks": "^3.1.0",
    "node-sass-import-once": "^1.2.0"
  },
  "paths": {
    "assets": "./public/assets",
    "tmp": "./tmp",
    "javascripts": {
      "src": [
        "./node_modules/jquery/dist/jquery.min.js",
        "./node_modules/moment/min/moment.min.js",
        "./node_modules/popper.js/dist/umd/popper.min.js",
        "./node_modules/bootstrap/dist/js/bootstrap.min.js",
        "./node_modules/tempusdominus-bootstrap-4/build/js/tempusdominus-bootstrap-4.min.js",
        "./node_modules/bootstrap-slider/dist/bootstrap-slider.min.js",
        "./node_modules/js-cookie/src/js.cookie.js",
        "./node_modules/simplemde/dist/simplemde.min.js",
        "./node_modules/angular/angular.js",
        "./node_modules/socket.io-client/dist/socket.io.js",
        "./node_modules/chocolat/dist/js/jquery.chocolat.min.js",
        "./node_modules/Croppie/croppie.min.js",
        "./node_modules/ng-img-crop/compile/minified/ng-img-crop.js",
        "./node_modules/jquery-dateFormat/dist/jquery-dateFormat.min.js",
        "./node_modules/select2/dist/js/select2.full.min.js",
        "./node_modules/fontawesome-iconpicker/dist/js/fontawesome-iconpicker.min.js"
      ],
      "dest": "application.js",
      "destmin": "application.min.js",
      "watch": [
        "./resources/assets/**/*.js"
      ]
    },
    "stylesheets": {
      "src": [
        "./resources/assets/sass/application.scss"
      ],
      "dest": "application.css",
      "destmin": "application.min.css",
      "watch": [
        "./resources/assets/**/*.scss"
      ]
    },
    "sass": {
      "src": [
        "./resources/assets/sass/framework/**/*.scss",
        "./resources/views/**/*.scss",
        "./node_modules/tempusdominus-bootstrap-4/build/css/tempusdominus-bootstrap-4.min.css",
        "./node_modules/bootstrap-slider/dist/css/bootstrap-slider.min.css",
        "./node_modules/simplemde/dist/simplemde.min.css",
        "./node_modules/font-awesome/css/font-awesome.css",
        "./node_modules/chocolat/dist/css/chocolat.css",
        "./node_modules/select2/dist/css/select2.min.css",
        "./node_modules/select2-bootstrap4-theme/dist/select2-bootstrap4.min.css",
        "./node_modules/fontawesome-iconpicker/dist/css/fontawesome-iconpicker.min.css"
      ],
      "dest": "./resources/assets/sass/components.scss",
      "watch": [
        "./resources/views/**/*.scss",
        "./resources/assets/**/*.scss"
      ]
    }
  }
}
